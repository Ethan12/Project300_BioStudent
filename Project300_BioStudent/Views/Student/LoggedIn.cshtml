
@{
    ViewBag.Title = "LoggedIn";
    
}

@if (Session["Id"] != null)
{
    <p class="text-success">@ViewBag.StatusMessage</p>
    <div>
        <h4>Student Management</h4>
        <hr />
        <dl class="dl-horizontal">
            <dt>Fingerprint:</dt>
            <dd>
                [
                @if (Convert.ToInt32(Session["FingerprintID"].ToString()) == -1)
                {
                    @Html.ActionLink("Enroll your fingerprint", "Enroll")
                }
                else
                {
                    @Html.Label("Fingerprint ID: " + Session["FingerprintID"].ToString()); 
                }
                ]
            </dd>
        </dl>
    </div>
}

